{
  "name": "webauthn-prf",
  "version": "1.0.0",
  "description": "A functional library for WebAuthn Pseudo-Random Function (PRF) key derivation with monadic patterns",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "dev": "rollup -c -w",
    "lint": "eslint src __tests__ --ext .ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run test"
  },
  "keywords": [
    "webauthn",
    "prf",
    "functional",
    "monadic",
    "cryptography",
    "passkey",
    "key-derivation"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "audit-ci": "^7.1.0",
    "eslint": "^9.29.0",
    "eslint-plugin-security": "^3.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rollup": "^4.14.0",
    "ts-jest": "^29.4.0",
    "typescript": "^5.4.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/webauthn-prf.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/webauthn-prf/issues"
  },
  "homepage": "https://github.com/your-username/webauthn-prf#readme",
  "dependencies": {
    "@simplewebauthn/browser": "^13.1.0",
    "neverthrow": "^8.2.0",
    "tslib": "^2.8.1"
  }
}
